# MySQL Database Configuration for TA-RDM Test Data Generator
# Copy this file to .env and fill in your actual values

# MySQL Connection
DB_HOST=localhost
DB_PORT=3308
DB_USER=ta_testdata
DB_PASSWORD=your_secure_password_here

# NOTE: Do NOT specify DB_NAME/database
# This generator uses schema-qualified table names (e.g., reference.ref_country)
# to access all 12 schemas: reference, party, tax_framework, registration,
# filing_assessment, accounting, payment_refund, compliance_control,
# document_management, withholding_tax, vat, income_tax

# Logging
LOG_LEVEL=INFO

# Data Generation
RANDOM_SEED=42

# ClickHouse Connection (Layer 3 Data Warehouse)
CH_HOST=localhost
CH_PORT=8123
CH_DATABASE=ta_dw
CH_USER=your_clickhouse_username
CH_PASSWORD=your_clickhouse_password
